<%= link_to 'Back to Employees', employees_path %> <br>
<h1>Customers</h1>

<table>
  <thead>
    <tr>
      <th><bold>Account Number</bold></th>
      <th><bold>Website</bold></th>
      <th><bold>Invoice Frequency</bold></th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.account_number %></td>
        <td><%= customer.website %></td>
        <td><%= customer.invoice_frequency %></td>
      </tr>
    <% end %>
  </tbody>
</table>
